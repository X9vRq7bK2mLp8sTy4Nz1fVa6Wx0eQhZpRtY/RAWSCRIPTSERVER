{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html"
    },
    {
      "src": "/raw",
      "dest": "/api/content.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/content.js"
    }
  ],
  "headers": [
    {
      "src": "/public/.*",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/admin",
      "headers": {
        "Content-Type": "text/html"
      }
    }
  ]
}
