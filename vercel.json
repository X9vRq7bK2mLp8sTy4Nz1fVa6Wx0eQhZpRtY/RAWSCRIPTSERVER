{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/content.js",
      "headers": {
        "Content-Type": "text/plain"
      }
    }
  ]
}
