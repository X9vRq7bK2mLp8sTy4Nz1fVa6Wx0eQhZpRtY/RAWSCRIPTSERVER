{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/admin",
      "dest": "/api/admin.js"
    },
    {
      "src": "/raw",
      "dest": "/api/content.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/content.js"
    }
  ]
}
