{
  "rewrites": [
    {
      "source": "/scripts/:scriptid",
      "destination": "/api/script.lua"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/public/:path*"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    }
  ]
}
